  border-top: 1rpx solid rgba(0,7,14,0.07);
  display: flex;
  align-items: center;
}

.schoolSearch{
  width:158rpx;
  height:59rpx;
  background:rgba(248,248,248,1);
  border-radius:30rpx;
  border:1rpx solid rgba(235,235,235,1);
  margin-left: 23rpx;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
}

.searchIcon{
  color:rgb(110,110,111);
}

.searchDes{
  font-size:28rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
