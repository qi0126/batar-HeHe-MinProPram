.tbwrap {
  margin: 0 auto;
  width: 690rpx;
  font-size: 20rpx;
  background: #fff;
  border: 1px solid #EDEDED;
  border-top: none
}

.tbtr {
  display: flex;
  border-top: 1px solid #EDEDED 
}

.tbtd:nth-of-type(1) {
  text-align: right;
  padding-right: 20rpx;
  line-height: 50rpx;
  width: 160rpx;
  color: #999;
  border-right: 1px solid #ededed
}

.tbtd:nth-of-type(2) {
  flex: 1;
  color: #333;
  line-height: 50rpx;
  margin-left: 20rpx;
}