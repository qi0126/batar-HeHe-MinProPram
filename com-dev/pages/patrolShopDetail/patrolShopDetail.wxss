/* 内容 */

.main {
    padding: 16rpx 15rpx 0rpx 11rpx;
    height: 100%;
}

.iconBigRed {
    color: #fff;
    background: #bf1d47;
    border-radius: 50%;
    width: 100rpx;
    height: 100rpx;
    font-size: 50rpx;
    text-align: center;
    line-height: 100rpx;
    position: fixed;
    right: 40rpx;
    bottom: 40rpx;
    box-shadow: 2px 2px 8px #d8d8d8;
}

.editShopTop {
    border-top: 1px solid #d8d8d8;
    display: flex;
    background: #fff;
    height: 100rpx;
    line-height: 100rpx;
    padding: 0 20rpx;
}

.shopReportTopOne {
    background: #fff;
    margin-top: 16rpx;
    padding: 20rpx 20rpx;
}

.titleOne {
    font-size: 28rpx;
    height: 80rpx;
    line-height: 80rpx;
    padding: 0 20rpx;
    font-weight: bold;
}

.titOne {
    border-left: 6rpx solid #bf1d47;
    padding-left: 20rpx;
}

.width30F {
    width: 30%;
    margin-right: 20rpx;
}

.fileUploadOne {
    width: 200rpx;
    height: 150rpx;
    float: left;
    margin-right: 20rpx;
    /* background: #9e0029; */
    border-radius: 10px;
}

.fileUploadTwo {
    width: 200rpx;
    height: 120rpx;
    float: left;
    margin-right: 20rpx;
    background: #f2f4f7;
    color: #9e0029;
    text-align: center;
    font-size: 24rpx;
    line-height: 40rpx;
    padding-top: 30rpx;
    font-weight: bold;
    border-radius: 10px;
}

.bottomOne {
    width: 320rpx;
    height: 80rpx;
    line-height: 80rpx;
    position: fixed;
    bottom: 40rpx;
    color: #bf1d47;
    left: 32rpx;
    font-size: 32rpx;
    border: 3rpx solid #bf1d47;
}

.bottomTwo {
    width: 320rpx;
    height: 80rpx;
    line-height: 80rpx;
    position: fixed;
    bottom: 40rpx;
    right: 32rpx;
    color: #fff;
    font-size: 32rpx;
    background: #bf1d47;
    border: 3rpx solid #bf1d47;
}

.txtRed {
    color: #9e0029;
}

.item {
    line-height: 28px;
    border-bottom: 1px solid #dedede;
    background: #fff;
    font-size: 28rpx;
    padding: 0 16px;
    font-weight: bold;
}

.item .txtBlack {
    line-height: 38rpx;
    margin-bottom: 20rpx;
}

.my_order {
    width: 14rpx;
    height: 28rpx;
}

.modeTwo {
    max-height: 500rpx;
    overflow: auto;
}

.userDivOne {
    color: #aaadb3;
    font-size: 28rpx;
    height: 87rpx;
    line-height: 87rpx;
}

.userDivOne .native {
    color: #242526;
}

.userDivOne .yesIconOne {
    float: right;
    padding-right: 30rpx;
}

.textareaDiv {
    background: #fff;
    margin-top: 16rpx;
    padding: 20rpx;
    font-size: 28rpx;
    height: 895rpx;
    font-weight: bold;
}

.shop_tit_left {
    width: initial;
}

.shop_tit_right {
