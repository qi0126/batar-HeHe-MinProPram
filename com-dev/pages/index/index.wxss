/* 百泰学堂 */

.school {
    background: rgba(246, 246, 246, 1);
}

.school .imgWrap {
    display: flex;
    align-items: center;
    justify-content: space-around;
}

.school .imgBlock {
    position: relative;
}

.imgBlock .infoTitle {
    position: absolute;
    left: 0rpx;
    top: 0rpx;
    right: 0rpx;
    bottom: 0rpx;
    font-size: 30rpx;
    font-weight: bold;
    color: rgba(255, 255, 255, 1);
    letter-spacing: 1rpx;
    text-shadow: 0px 1rpx 2rpx rgba(9, 31, 81, 0.28);
    display: flex;
    justify-content: center;
    align-items: center;
}

.imgBlock .infoActive {
    font-size: 36rpx;
}

.school .titleWrap {
    padding: 92rpx 0 42rpx 0;
    background: rgba(249, 249, 249, 1);
}

.school .header {
    box-sizing: border-box;
    padding: 14rpx 10rpx 0rpx 10rpx;
    min-height: 442rpx;
}

.school .headerImg {
    width: 320rpx;
    height: 320rpx;
}

.school .active {
    transition-duration: 0.3s;
    transition-timing-function: linear;
    width: 366rpx;
    height: 366rpx;
}

.school .iconWrap {
    display: flex;
    align-items: center;
    margin-top: 16rpx;
}

.school .iconBlock {
    flex: 1;
    text-align: center;
}

.icon-sanjiaoxing {
    opacity: 1;
    transition-duration: 0.3s;
    transition-timing-function: linear;
}

.school .liWrap {
    box-sizing: border-box;
    padding: 30rpx 9rpx 42rpx 30rpx;
    /* background: gray; */
}

.school .schoolTitle {
    font-size: 32rpx;
    /* font-weight: 600; */
    color: rgba(0, 0, 0, 1);
    margin-bottom: 48rpx;
}

.infoWrap {
    opacity: 1;
    transition-property: opacity;
    transition-duration: 0.1s;
    transition-timing-function: linear;
}

.infoWrap .lastLi {
    display: flex;
    justify-content: flex-end;
    align-items: center;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(225, 103, 29, 1);
    margin-top: -23rpx;
}

.lastLi .arrow {
    font-size: 18rpx;
}

/* 知识课堂 */

.knowledgeWrap {
    position: relative;
    z-index: 2;
    transition-duration: 0.1s;
    transition-timing-function: linear;
}

.knowledgeWrap .li {
    display: flex;
    justify-content: space-between;
    border-bottom: 1rpx solid rgba(224, 224, 224, 1);
    padding-bottom: 45rpx;
    margin-bottom: 43rpx;
    
}

.knowledgeWrap .des {
    width: 476rpx;
    font-size: 26rpx;
    font-weight: bold;
    color: rgba(51, 51, 51, 1);
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.knowledgeWrap .preview {
    width: 110rpx;
    height: 44rpx;
    background: rgba(225, 103, 29, 1);
    border-radius: 2rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(255, 255, 255, 1);
    text-align: center;
    line-height: 44rpx;
    cursor: pointer;
    margin-right: 30rpx;
}

.knowledgeWrap .lastLi {
    display: flex;
    justify-content: flex-end;
    align-items: center;
    font-size: 24rpx;
    font-weight: 400;
    color: rgba(225, 103, 29, 1);
    margin-top: -20rpx;
    padding-right: 17rpx;
}

